{
  "name": "dashing-widgets",
  "version": "0.1.0",
  "description": "Create dashboards with widgets easily",
  "author": {
    "name": "Remko Klein"
  },
  "license": "MIT",
  "main": "dist/backend/app",
  "scripts": {
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:frontend\"",
    "dev:server": "ts-node-dev --no-notify src/backend/app",
    "dev:frontend": "NODE_ENV=development webpack-dev-server --mode development --history-api-fallback",
    "build:server": "tsc && mv dist/src/* dist && rm -r dist/src",
    "build:frontend": "NODE_ENV=production webpack --mode production",
    "build": "rm -rf dist/* && yarn build:server && yarn build:frontend",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "docs": "typedoc --theme minimal --out docs ./typed_config.ts"
  },
  "dependencies": {
    "apexcharts": "^3.10.1",
    "bottleneck": "^2.19.5",
    "bull": "^3.12.0",
    "classnames": "^2.2.6",
    "config": "^3.2.4",
    "dayjs": "^1.8.17",
    "express": "^4.17.1",
    "graphql-request": "^1.8.2",
    "http-proxy-middleware": "^1.0.0",
    "ioredis": "^4.16.0",
    "node-fetch": "^2.6.1",
    "object-hash": "^2.0.3",
    "react": "^16.11.0",
    "react-apexcharts": "^1.3.3",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "require-all": "^3.0.0",
    "simple-oauth2": "^3.3.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/bull": "^3.10.6",
    "@types/classnames": "^2.2.9",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.2",
    "@types/node-fetch": "^2.5.3",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/simple-oauth2": "^2.5.2",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/webpack": "^4.39.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.0",
    "terser-webpack-plugin": "^3.0.7",
    "ts-node-dev": "^1.0.0-pre.44",
    "typedoc": "^0.17.4",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
