{
  "name": "dashing-widgets",
  "version": "0.1.0",
  "description": "Create dashboards with widgets easily",
  "author": {
    "name": "Remko Klein"
  },
  "license": "MIT",
  "main": "dist/backend/app",
  "scripts": {
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:frontend\"",
    "dev:server": "ts-node-dev --no-notify src/backend/app",
    "dev:frontend": "NODE_ENV=development webpack serve --mode development --history-api-fallback",
    "build:server": "tsc && mv dist/src/* dist && rm -r dist/src",
    "build:frontend": "NODE_ENV=production webpack --mode production",
    "build": "rm -rf dist/* && yarn build:server && yarn build:frontend",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "docs": "typedoc --theme minimal --out docs ./typed_config.ts"
  },
  "dependencies": {
    "apexcharts": "^3.10.1",
    "bottleneck": "^2.19.5",
    "bull": "^3.12.0",
    "classnames": "^2.2.6",
    "config": "^3.2.4",
    "cross-fetch": "^3.0.6",
    "dayjs": "^1.8.17",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-request": "^3.1.0",
    "http-proxy-middleware": "^1.0.0",
    "ioredis": "^4.16.0",
    "node-fetch": "^2.6.0",
    "object-hash": "^2.0.3",
    "react": "^17.0.1",
    "react-apexcharts": "^1.3.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.1.2",
    "require-all": "^3.0.0",
    "simple-oauth2": "^4.2.0",
    "socket.io": "^4.0.0",
    "socket.io-client": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/bull": "^3.10.6",
    "@types/classnames": "^2.2.9",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.2",
    "@types/node-fetch": "^2.5.3",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/simple-oauth2": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typedoc": "^0.20.32",
    "typescript": "^4.0.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
